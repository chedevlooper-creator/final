{
  "name": "Yardım Yönetim Paneli",
  "description": "Sivil toplum kuruluşları için kapsamlı yardım yönetim sistemi",
  "repository": "https://github.com/Kafkasportal/Final-panel",
  "logo": "https://raw.githubusercontent.com/vercel/next.js/main/docs/public/logo.svg",
  "keywords": ["nextjs", "supabase", "yardım", "stk", "typescript"],
  "success_url": "/",
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": {
      "description": "Supabase Project URL",
      "required": true
    },
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": {
      "description": "Supabase Anonymous Key",
      "required": true
    },
    "NODE_ENV": {
      "value": "production"
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
